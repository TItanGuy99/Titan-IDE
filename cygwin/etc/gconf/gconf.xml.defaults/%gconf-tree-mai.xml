<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="system">
			<dir name="smb">
				<entry name="workgroup">
					<local_schema short_desc="एसएमबी वर्कग्रुप">
						<longdesc>Windows सँजाल कार्यसमूह अथवा डोमेन जकर प्रयोक्ता हिस्सा अछि. नवीन कार्यसमूह क&apos; पूर्ण रूपेँ प्रभावी हएबाक लेल प्रयोक्ता केँ लाग आउट आ लाग बैक हएबाक जरूरत अछि.</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="proxy">
				<entry name="autoconfig_url">
					<local_schema short_desc="स्वचालित प्रोक्सी बिन्यास यूआरएल">
						<longdesc>यूआरएल जे प्राक्सी बिन्यास मानसभकेँ प्रदान करैत अछि.</longdesc>
					</local_schema>
				</entry>
				<entry name="socks_port">
					<local_schema short_desc="साक्स प्राक्सी पोर्ट">
						<longdesc>मशीन पर पोर्ट पारिभाषित अछि &quot;/system/proxy/socks_host&quot; क&apos; द्वारा जकरा द्वारा अहाँ प्राक्सी करैत अछि.</longdesc>
					</local_schema>
				</entry>
				<entry name="socks_host">
					<local_schema short_desc="साक्स प्राक्सी होस्ट नाम">
						<longdesc>मशीन नाम जकरासँ प्राक्सी साक्स कएनाइ अछि.</longdesc>
					</local_schema>
				</entry>
				<entry name="ftp_port">
					<local_schema short_desc="एफ़टीपी प्रोक्सी पोर्ट">
						<longdesc>मशीन पर पोर्ट पारिभाषित अछि &quot;/system/proxy/ftp_host&quot; क&apos; द्वारा जकरा द्वारा अहाँ प्राक्सी करैत अछि.</longdesc>
					</local_schema>
				</entry>
				<entry name="ftp_host">
					<local_schema short_desc="एफ़टीपी प्रोक्सी होस्ट नाम">
						<longdesc>मशीन नाम जकरासँ प्राक्सी एफटीपी कएनाइ अछि.</longdesc>
					</local_schema>
				</entry>
				<entry name="secure_port">
					<local_schema short_desc="सुरक्षित एचटीटीपी प्राक्सी पोर्ट">
						<longdesc>मशीन पर पोर्ट पारिभाषित अछि &quot;/system/proxy/secure_host&quot; क&apos; द्वारा जकरा द्वारा अहाँ प्राक्सी करैत अछि.</longdesc>
					</local_schema>
				</entry>
				<entry name="secure_host">
					<local_schema short_desc="सुरक्षित एचटीटीपी प्राक्सी होस्ट नाम">
						<longdesc>मशीन नाम जकरासँ प्रॉक्सी सिक्योर एचटीटीपी कएनाइ अछि.</longdesc>
					</local_schema>
				</entry>
				<entry name="mode">
					<local_schema short_desc="प्रॉक्सी बिन्यास मोड">
						<longdesc>प्राक्सी बिन्यासक मोड चुनू. समर्थित मूल्य अछि &quot;किछु नहि&quot;, &quot;हस्तचालित&quot;, &quot;स्वचालित&quot;.</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="http_proxy">
				<entry name="ignore_hosts">
					<local_schema short_desc="नान-प्राक्सी होस्टसभ">
						<longdesc>ई कुँजीमे मेजबानसभक सूची सामिल अछि जे सीधे जुड़ल अछि. बजाए प्राक्सी सँ भ&apos; कए (जँ ई सक्रिय अछि). मान मेजबाननाम, डोमेन (*.foo.com क तरह क&apos; आरंभिक वाइल्डकार्ड), IP मेजबान पता (दुनू IPv4 आओर IPv6) आओर नेटमास्क क&apos; सँग सँजाल पता (किछु ई तरह क&apos; 192.168.0.0/24).</longdesc>
					</local_schema>
				</entry>
				<entry name="authentication_password">
					<local_schema short_desc="एचटीटीपी प्राक्सी कूटशब्द">
						<longdesc>जखन एचटीटीपी प्राक्सी कए रहल अछि तँ प्रामाणीकरण लेल पास करबाक लेल कूटशब्द.</longdesc>
					</local_schema>
				</entry>
				<entry name="authentication_user">
					<local_schema short_desc="एचटीटीपी प्रॉक्सी प्रयोक्ता नाम">
						<longdesc>जखन एचटीटीपी प्राक्सी कए रहल छी तँ प्रामाणीकरण लेल पास करबाक लेल प्रयोक्ता नाम.</longdesc>
					</local_schema>
				</entry>
				<entry name="use_authentication">
					<local_schema short_desc="प्राक्सी सर्वर कनेक्शन्स प्रामाणित करू">
						<longdesc>जँ सही अछि तखन प्रॉक्सी सर्वरमे संबंधन क&apos; लेल सत्यापनक जरूरत होइछ. प्रयोक्तानाम/कूटशब्द कोंबो केँ &quot;/system/http_proxy/authentication_user&quot; आओर &quot;/system/http_proxy/authentication_password&quot; क&apos; द्वारा परिभाषित हए छै.</longdesc>
					</local_schema>
				</entry>
				<entry name="port">
					<local_schema short_desc="एचटीटीपी प्रॉक्सी पोर्ट">
						<longdesc>मशीन पर पोर्ट पारिभाषित अछि &quot;/system/http_proxy/host&quot; क&apos; द्वारा जकरा द्वारा अहाँ प्रॉक्सी करैत अछि.</longdesc>
					</local_schema>
				</entry>
				<entry name="host">
					<local_schema short_desc="एचटीटीपी प्रोक्सी होस्ट नाम">
						<longdesc>मशीन नाम जकरासँ प्राक्सी एचटीटीपी कएनाइ अछि.</longdesc>
					</local_schema>
				</entry>
				<entry name="use_http_proxy">
					<local_schema short_desc="एचटीटीपी प्राक्सी प्रयोग करू">
						<longdesc>जखन इंटरनेट पर एचटीटीपी एक्सेस कएल जाए तँ प्रोक्सी बिन्यास सक्षम करू.</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="dns_sd">
				<entry name="extra_domains">
					<local_schema short_desc="DNS-SD सेवामे देखबाक लेल अतिरिक्त डोमेन">
						<longdesc>DNS-SD डोमेन क&apos; कोमा सँ अलग सूची जकरा &quot;network:///&quot; स्थानमे दृश्य होनाइ चाही.</longdesc>
					</local_schema>
				</entry>
				<entry name="display_local">
					<local_schema short_desc="स्थानीय DNS-SD सेवा कहिना देखाएनाइ अछि">
						<longdesc>संभावित मान अछि &quot;merged&quot;, &quot;separate&quot; आओर &quot;disabled&quot;.</longdesc>
					</local_schema>
				</entry>
			</dir>
		</dir>
		<dir name="desktop">
			<dir name="gnome">
				<dir name="typing_break">
					<entry name="enabled">
